{
  "name": "js-create-element",
  "version": "1.2.4",
  "description": "Library to create a DOM element with inline CSS using pure JS",
  "scripts": {
    "test": "./node_modules/.bin/nyc mocha --require babel-core/register --require test/utils/helpers.js test/*.js",
    "test:ci": "npm test -- --watch",
    "build": "webpack --config webpack.config.js",
    "lint": "./node_modules/.bin/eslint src/",
    "prettier": "./node_modules/.bin/prettier --single-quote --trailing-comma es5 --write '{,!(node_modules|docs|build)/**/}*.js'",
    "docs": "./node_modules/.bin/jsdoc -d docs/ src/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/athersharif/js-create-element.git"
  },
  "keywords": [
    "js",
    "element",
    "css",
    "pseudo",
    "pure"
  ],
  "author": "Ather Sharif",
  "license": "MIT",
  "main": "index.js",
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.14.0",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.5.1",
    "mocha": "^4.1.0",
    "nyc": "^11.4.1",
    "prettier": "^1.9.2",
    "webpack": "^3.10.0"
  }
}
